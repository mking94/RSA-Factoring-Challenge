#!/bin/bash
vr=$(echo "$@" | awk '{print NF}')
file=$(sudo cat tests/test00)
if [ "$vr" -eq "0" ]
then
echo "Please add file path as argument"
exit 1
fi
if [ "$vr" -eq "1" ]
then
if [ -f $1 ]
then
echo "good"
else
echo "Please insert corret file path"
fi
fi
if [ "$vr" -gt "1" ]
then
echo "Please setin only one argument"
echo $file | head -n 1
exit 1
fi
echo "end"
